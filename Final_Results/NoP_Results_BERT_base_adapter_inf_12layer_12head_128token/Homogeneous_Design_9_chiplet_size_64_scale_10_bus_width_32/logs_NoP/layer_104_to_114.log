//8X8 mesh with 1 flits per packet under injection mode
//injection rate here is packet per cycle, NOT flit per cycle

// Topology

topology = mesh;
//k=16; //trace_based
k=3;
n=2;

// Routing
routing_function = dor;

in_ports = 5;
out_ports = 5;

// Custom Traffic
//traffic = custom;
//injection_process = custom;

// Trace-based Traffic
traffic = trace_based;
injection_process = trace_based;

// Constant delays
routing_delay = 1;
vc_alloc_delay = 1;
sw_alloc_delay = 1;
st_final_delay = 1;

packet_size = 1;


// Simulation
sim_type = latency;
sample_period = 100000;
warmup_periods = 0;
max_samples = 1;

injection_rate = 0.01;

num_vcs = 3;

buf_size = 10;
vc_buf_size = 10;
output_buffer_size = 1;

private_bufs = 1;
private_buf_size = 1;

sim_power = 1;

tech_file = techfile_nop.txt;

channel_width = 32;

//watch_flits = watch_flits.txt;
//watch_file = watch_file.txt;
//watch_out = watch_out.txt;

